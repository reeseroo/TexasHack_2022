{
    "manifest_version": 3,
    "name": "Text Filter",
    "version": "1.0.0",
  
    "description": "Filter out the garbage found within the stuff you read online!",

    "action": {
        "default_popup": "filter.html"
    },

    "background": {
        "service_worker": "background.js"
    },

    "permissions": [
        "activeTab"
    ]

    }